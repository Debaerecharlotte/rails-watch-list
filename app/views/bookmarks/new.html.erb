<div class="container mt-3">
  <div class="page-main-header">
    <h3>Adding a movie to List</h3>
  </div>

  <div class="form-new-list">
    <%= simple_form_for([@list, @bookmark]) do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
