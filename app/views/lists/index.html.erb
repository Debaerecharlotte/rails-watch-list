<div class="container mt-3">
  <div class="page-main-header">
    <h3>My Movie Lists</h3>
  </div>

  <div class="cards">
    <% @lists.each do |list| %>
        <div class="card-category" style="background-image: url('<%= cl_image_path list.photo.key, crop: :fill %>')">
          <%= link_to list.name, list_path(list) %>
        </div>
    <% end %>
  </div>
</div>
