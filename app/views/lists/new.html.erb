<div class="container mt-3">
  <div class="page-main-header">
    <h3>Create a new list</h3>
  </div>

  <div class="form-new-list">
    <%= simple_form_for(@list) do |f| %>
      <%= f.input :name, placeholder: 'list name' %>
      <%= f.input :photo, as: :file %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
